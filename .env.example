# TicketHawk Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3000
NODE_ENV=development

# Database (PostgreSQL) - Required for production
DATABASE_URL=postgresql://username:password@localhost:5432/tickethawk

# JWT Authentication
JWT_SECRET=your_jwt_secret_here

# API Keys
# Get your Ticketmaster key at: https://developer.ticketmaster.com/
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here

# Get your SeatGeek Client ID at: https://seatgeek.com/account/develop
SEATGEEK_CLIENT_ID=your_seatgeek_client_id_here

# Get your StubHub keys at: https://developer.stubhub.com/
STUBHUB_APP_KEY=your_stubhub_app_key_here
STUBHUB_API_KEY=your_stubhub_api_key_here
